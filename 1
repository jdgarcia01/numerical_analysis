#include <stdio.h>
#include <math.h>

// ODE to solve.
float func(float t, float y){

	return( cos(2*t) + sin(3 * t));

}

float exact_func(float t, float y){

	return( 1/2 * sin(2 * t) - 1/3 * cos(3 * t) + 4/3 );


}
int main(){

	double a,b,N;
	double alpha;

	printf("a endpoint\n");
	scanf("%lf",&a);

	printf("b endpoint\n");
	scanf("%lf",&b);

	printf("intial condition Alpha: \n");
	scanf("%lf",&alpha);

	printf("iterations N:\n");
	scanf("%lf",&N);

	double h;
	printf("h: \n");
	scanf("%lf",&h);

	//double h = (b - a)/N; // Determine step size.
	printf("h is: %lf\n",h);

	double t = a;  
	double w = alpha;
	
	
	for(int i = 1; i <= N; i++){

		w = w + h * func(t,w); // compute wi
		t = a + (i * h);          // compute ti
		printf(" Approximate t is: %lf w is: %lf\n",t , w);
		printf(" Exact solution is: %lf\n", func
	}

	return 0;


}
